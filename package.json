{
  "name": "webpack-express-typescript",
  "version": "1.0.0",
  "engines": {
    "node": "12.x"
  },
  "description": "Easy starter project for writing modern Express Applications using TypeScript, and Webpack",
  "private": true,
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "node node_modules/.bin/webpack --config config/webpack.config.js",
    "start": "npm run build && node dist/compiled",
    "dev": "./node_modules/.bin/nodemon"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      "app.ts",
      "server/",
      "config/",
      "bin/"
    ],
    "ext": "js, json, ts",
    "ignore": [
      "**/__tests__/**",
      "*.test.*",
      "*.spec.*"
    ]
  },
  "main": "",
  "keywords": [],
  "author": "Danny",
  "license": "MIT",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.7",
    "@types/morgan": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.3.12"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "pug": "^3.0.0"
  }
}